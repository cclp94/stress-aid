{
  "manifest_version": 2,

  "name": "Stress Aid",
  "description": "This extension will help you make your days feel a lot more better",
  "version": "1.0",

  "options_page": "templates/options.html",
  "browser_action": {
   "default_icon": "static/img/icon.png",
   "default_popup": "templates/index.html"
  },
  "permissions": [
   "activeTab"
   ]
}